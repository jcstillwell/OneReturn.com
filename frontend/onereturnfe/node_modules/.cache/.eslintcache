nfe\\src\\App.js",["125","126","127","128","129","130"],[],"import React, { useState, useEffect, useContext } from \"react\";\nimport { Route, Redirect, Routes, useNavigate, useLocation} from 'react-router-dom';\nimport { SidebarContext } from \"./context/SidebarContext.js\";\nimport Sidebar from \"./pages/app/Sidebar\";\nimport Navbar from './pages/nav/Navbar.js'\n\nimport AnimatedRoutes from \"./pages/nav/AnimatedRoutes.js\"\n\nconst App = () => {\n\n  const [sidebarState, dispatch] = useContext(SidebarContext);\n  const changeDash = (type) => {dispatch({type})};\n  let navigate = useNavigate();\n\n  const { pathname } = useLocation();\n  const settingsDashPages = ['/settings', '/settings/user','/settings/receipts'];\n  const walletDashPages = ['/wallet', '/shared', 'returns'];\n\n  useEffect(() => {\n    if(walletDashPages.includes(pathname)){\n      dispatch({type: 'SHOW_BAR'})\n      dispatch({type: 'WALLET_BAR'});\n    }\n    else if(settingsDashPages.includes(pathname)){\n      dispatch({type: 'SHOW_BAR'})\n      dispatch({type: 'SETTINGS_BAR'});\n    }\n    else if(pathname === '/'){\n      navigate('/wallet')\n    } else {\n      dispatch({type: 'HIDE_BAR'});\n    }\n  }, [pathname]);\n\n  return (\n    <div className=\"App\">\n      <div className=\"app-container\">\n        <div className=\"nav\">\n          <Navbar/>\n        </div>\n        <div className=\"dashboard\">\n          {sidebarState.visible && (\n            <Sidebar/>\n          )}\n        </div>\n        <div className={sidebarState.visible ? \"app-content-dashboard\" : \"app-content\"}>\n          <AnimatedRoutes />\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\Jackson\\Desktop\\OR-Localsave\\frontend\\onereturnfe\\src\\reportWebVitals.js",[],[],"C:\\Users\\Jackson\\Desktop\\OR-Localsave\\frontend\\onereturnfe\\src\\context\\SidebarContext.js",["131"],[],"import { React ,createContext, useState, useReducer} from \"react\";\n\nexport const SidebarContext = createContext();\n\nexport const SidebarProvider = ({ children }) => {\n    const initialState = { type: \"DEFAULT_BAR\", visible: false };\n\n    const reducer = (state, action) => {\n        switch(action.type) {\n            case 'SETTINGS_BAR':\n                return { ...state, type: \"SETTINGS_BAR\"};\n            case 'WALLET_BAR':\n                return { ...state, type: \"WALLET_BAR\"};\n            case 'SHOW_BAR':\n                return { ...state, visible: true};\n            case 'HIDE_BAR':\n                return { ...state, visible: false};\n            default:\n                return state;\n        }\n    }\n\n    const [sidebarState, dispatch] = useReducer(reducer, initialState);\n\n    return (\n        <SidebarContext.Provider value={[sidebarState, dispatch]}>\n            {children}\n        </SidebarContext.Provider>\n    );\n}","C:\\Users\\Jackson\\Desktop\\OR-Localsave\\frontend\\onereturnfe\\src\\context\\AuthContext.js",[],[],"C:\\Users\\Jackson\\Desktop\\OR-Localsave\\frontend\\onereturnfe\\src\\pages\\nav\\Navbar.js",["132","133","134","135","136","137"],[],"import React, { useState, useEffect, useContext, useRef } from \"react\";\nimport { Link, useNavigate } from \"react-router-dom\";\nimport { AuthContext } from \"../../context/AuthContext.js\";\nimport Cookies from 'js-cookie';\nimport { Button } from \"./Button.js\";\nimport Dropdown from \"../app/Dropdown.js\";\nimport './styles/nav.css';\n\nconst Navbar = () => {\n\n    //auth\n    const { user, dispatch } = useContext(AuthContext);\n    const isAuthenticated = !!user;\n\n    //util\n    const [click, setClick] = useState(false);\n    const [dropdown, setDropdown] = useState(false);\n    let navigate = useNavigate();\n\n    const handleClick = () => setClick(!click);\n    const toggleDropdown = () => setDropdown(!dropdown);\n\n    return (\n        <nav className=\"nav\">\n            <div className=\"navbar-logo\">\n                <h3>OneReturn</h3>\n            <